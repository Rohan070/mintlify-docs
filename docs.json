{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "maple",
  "name": "AUM Docs",
  "colors": {
    "primary": "#d6bd13",
    "light": "#181818",
    "dark": "#fff"
  },
  "appearance": {
    "default": "dark",
    "strict": true
  },
  "favicon": "/favicon.ico",
  "navigation": {
    "tabs": [
      {
        "tab": "Guides",
        "groups": [
          {
            "group": "Get Started",
            "pages": [
              "introduction",
              "quickstart",
              "development"
            ]
          },
          {
            "group": "Essentials",
            "pages": [
              "essentials/markdown",
              "essentials/code",
              "essentials/images",
              "essentials/settings",
              "essentials/navigation",
              "essentials/reusable-snippets"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Documentation",
          "href": "https://docs.aumbot.co",
          "icon": "book-open-cover"
        },
        {
          "anchor": "Community",
          "href": "https://discord.gg/msECKzvm",
          "icon": "discord"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.png",
    "dark": "/logo/dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "mailto:support@aumbo.co"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Home",
      "href": "https://aumbot.co"
    }
  },
  "footer": {
    "socials": {
      "linkedin": "https://www.linkedin.com/company/aumbot/"
    }
  }
}